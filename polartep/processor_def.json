{
  "dockerimage": "test/ai4artic_snow:0.1",
  "title": "AI4Artic - snow",
  "description": "Estimate FSC from S3 data using deep learning",
  "inputs": [
    {
      "identifier": "date",
      "title": "Date to process",
      "min_occurs": 1,
      "max_occurs": 1,
      "args": ["{date}"],
      "literal": {
        "type":["str"],
        "max_size": 5000
      }
    }
  ],
  "outputs": [
    {
      "identifier": "fsc",
      "title": "GeoTiff with Fractional Snow Cover",
      "gs_publish": false,
      "file": {
        "mime_types": ["image/gtif"],
        "pattern": "fsc.tiff",
        "max_size": 5000
      }
    }
  ]
}
